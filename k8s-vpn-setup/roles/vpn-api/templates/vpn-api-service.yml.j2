apiVersion: v1
kind: Service
metadata:
  name: vpn-backend
  namespace: vpn
spec:
  selector:
    app: vpn-backend
  ports:
  - port: 8000
    targetPort: 8000
  type: LoadBalancer
---
apiVersion: v1
kind: Service
metadata:
  name: postgres
  namespace: vpn
spec:
  selector:
    app: postgres
  ports:
  - port: 5432
    targetPort: 5432
  type: ClusterIP